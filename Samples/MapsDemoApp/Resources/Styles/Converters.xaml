<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:c="clr-namespace:ValueConverters;assembly=ValueConverters"
    xmlns:converters="clr-namespace:Superdev.Maui.Maps.Converters;assembly=Superdev.Maui.Maps">

    <c:BoolInverter x:Key="BoolInverter" />

    <converters:DistanceToStringConverter x:Key="DistanceToStringConverter" />

    <c:BoolToImageSourceConverter
        x:Key="BoolToPushPinImageSourceConverter"
        FalseValue="parking_lot_pin"
        TrueValue="parking_lot_pin_selected" />

    <c:NullToBoolConverter
        x:Key="IsNotNullToBoolConverter"
        IsInverted="True" />
    <c:StringIsNotNullOrEmptyConverter x:Key="StringIsNotNullOrEmptyConverter" />

</ResourceDictionary>